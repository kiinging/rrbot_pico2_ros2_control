      <origin xyz="0.005000000000000005 -0.008097752979871468 0.024382343749636937" rpy="0 0 0"/>
      <mass value="0.2557057360993377"/>
      <inertia ixx="6.9e-05" iyy="4.6e-05" izz="4e-05" ixy="0.0" iyz="-1e-06" ixz="-0.0"/>
